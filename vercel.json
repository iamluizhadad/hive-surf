{
  "version": 2,
  "builds": [
    {
      "src": "quasar.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/spa"
      },
      "env": {
        "NODE_ENV": "production"
      },
      "output": {
        "clean": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
